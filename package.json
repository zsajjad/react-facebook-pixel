{
  "name": "react-facebook-pixel",
  "version": "1.0.5",
  "description": "Pixel Kit for React",
  "main": "lib/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development&& webpack-dev-server --config webpack.config.dev.js",
    "bundle": "cross-env NODE_ENV=production&& webpack -p --config webpack.config.dist.js",
    "lint": "eslint --ignore-path .gitignore --fix",
    "lint:staged": "lint-staged",
    "clean": "rm -rf node_modules",
    "prettify": "prettier --write",
    "prepare": "cross-env NODE_ENV=production&& babel src --out-dir ./lib --source-maps"
  },
  "files": [
    "src",
    "lib",
    "dist"
  ],
  "repository": "git://github.com/zsajjad/react-facebook-pixel.git",
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "angular",
    "vue",
    "pixel",
    "facebook-pixel"
  ],
  "author": {
    "name": "Zain Sajjad",
    "email": "zsajjad93@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zsajjad/react-facebook-pixel/issues"
  },
  "homepage": "https://github.com/zsajjad/react-facebook-pixel",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.7.5",
    "@babel/plugin-transform-react-constant-elements": "^7.7.4",
    "@babel/plugin-transform-react-inline-elements": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "autoprefixer": "9.7.3",
    "babel-eslint": "10.0.3",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.3",
    "eslint": "6.7.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "webpack": "4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "3.9.0"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "git add --force"
    ],
    "*.json": [
      "yarn prettify",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged"
}
